# 
# 10 / 3 + 6 = 3 + 6 = 9
# 10 / 3 + 6 = 3 + 6 = 9
# 10 / 3 + 5 = 3 + 5 = 8
# 
# 9 + 9 + 8 / 3 = 8 (-2)
# 
# 8 - 6 = 2
# 8 - 6 = 2
# 8 - 5 = 3
